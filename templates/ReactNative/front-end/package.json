/*
path: package.json
completePath: front-end/package.json
unique_id: GFoENpEW
*/
{
  "name": "{{ application.settings.name | lower | friendly }}",
  {% set version = insert_setting('version') %}
  {% if version %}
  "version": "{{ version }}",
  {% else %}
  "version": "1.0.0",
  {% endif %}
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web"
    {% set npmScripts = insert_setting('npmScripts') %}
    {% if npmScripts %},{{ insert_setting('npmScripts') | raw }}{% endif %}
  },
  "dependencies": {
    "@react-native/babel-preset": "^0.73.21",
    "@react-native/metro-babel-transformer": "^0.73.15",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "axios": "^1.4.0",
    "expo-build-properties": "~0.8.3",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "expo": "^49.0.21",
    "expo-status-bar": "~1.6.0",
    "immer": "^10.0.2",
    "react": "18.2.0",
    "react-native": "0.72.10",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-gradle-plugin": "^0.71.19",
    "react-native-paper": "^5.8.0",
    "react-native-reanimated": "~3.5.4",
    "react-native-vector-icons": "^10.0.3",
    "react-native-safe-area-context": "4.6.3",
    "react-native-sass-transformer": "^2.0.0",
    "react-native-screens": "~3.22.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "redux-observable": "^2.0.0",
    "sass": "^1.69.5",
    "typesafe-actions": "^5.1.0",
    "expo-image-picker": "~14.3.2",
    {{ insert_setting('Packages') | raw }}
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    {{ insert_setting('DevPackages') | raw }}
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0-0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0-0",
    "@babel/plugin-transform-arrow-functions": "^7.0.0-0",
    "@babel/plugin-transform-shorthand-properties": "^7.0.0-0",
    "@babel/plugin-transform-template-literals": "^7.0.0-0"
  },
  "private": true
}
