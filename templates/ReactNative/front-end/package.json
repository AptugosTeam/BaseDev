    "react-native-gesture-handler": "~2.9.0",
    "react-redux": "^8.0.5",
    "react-native-paper": "^5.8.0",
    "redux": "^4.2.1",
    "axios": "^1.4.0",
    "redux-observable": "^2.0.0",
    "react-native-select-dropdown": "^3.3.3",
    "typesafe-actions": "^5.1.0",
